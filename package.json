{
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node .",
    "local": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "dev": "rm -f .env && cp .env.development .env && ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "qa": "rm -f .env && cp .env.qa .env && ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "uat": "rm -f .env && cp .env.uat .env && ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "prod": "rm -f .env && cp .env.production .env && ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "tslint": "tslint --project tsconfig.json"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "prisma": "^6.18.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv-flow": "^3.3.3",
    "@types/morgan": "^1.9.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv-flow": "^4.1.0",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "i18n-nodejs": "^4.1.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "pino": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.2.1"
  }
}
